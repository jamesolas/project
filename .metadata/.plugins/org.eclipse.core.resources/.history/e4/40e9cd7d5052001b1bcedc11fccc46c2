package com.app.model;

public class Transactions {
	private long sendingAccountNumber;
	private long receivingAccountNumber;
	private long amount;
	private long date;
	
	public Transactions() {
		// TODO Auto-generated constructor stub
	}

	public Transactions(long sendingAccountNumber, long receivingAccountNumber, long amount, long date) {
		super();
		this.sendingAccountNumber = sendingAccountNumber;
		this.receivingAccountNumber = receivingAccountNumber;
		this.amount = amount;
		this.date = date;
	}

	public long getSendingAccountNumber() {
		return sendingAccountNumber;
	}

	public void setSendingAccountNumber(long sendingAccountNumber) {
		this.sendingAccountNumber = sendingAccountNumber;
	}

	public long getReceivingAccountNumber() {
		return receivingAccountNumber;
	}

	public void setReceivingAccountNumber(long receivingAccountNumber) {
		this.receivingAccountNumber = receivingAccountNumber;
	}

	public long getAmount() {
		return amount;
	}

	public void setAmount(long amount) {
		this.amount = amount;
	}

	public long getDate() {
		return date;
	}

	public void setDate(long date) {
		this.date = date;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + (int) (amount ^ (amount >>> 32));
		result = prime * result + (int) (date ^ (date >>> 32));
		result = prime * result + (int) (receivingAccountNumber ^ (receivingAccountNumber >>> 32));
		result = prime * result + (int) (sendingAccountNumber ^ (sendingAccountNumber >>> 32));
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Transactions other = (Transactions) obj;
		if (amount != other.amount)
			return false;
		if (date != other.date)
			return false;
		if (receivingAccountNumber != other.receivingAccountNumber)
			return false;
		if (sendingAccountNumber != other.sendingAccountNumber)
			return false;
		return true;
	}

	@Override
	public String toString() {
		return "Transactions [sendingAccountNumber=" + sendingAccountNumber + ", receivingAccountNumber="
				+ receivingAccountNumber + ", amount=" + amount + ", date=" + date + "]";
	}
	
	
}
